<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lab.entity.Project" table="project">
		<id name="Prid" column="Prid">
			<generator class="native" />
		</id>
		<property name="PrTheme" column="PrTheme" />
		<property name="PrDescription" column="PrDescription" />
		
	</class>
</hibernate-mapping>
