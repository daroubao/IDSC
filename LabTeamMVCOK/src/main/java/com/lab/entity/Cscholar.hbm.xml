<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lab.entity.Cscholar" table="cscholar">
		<id name="CSid" column="CSid">
			<generator class="native" />
		</id>
		<property name="CSname" column="CSname" />
		<property name="CSintroduce" column="CSintroduce" />
		<property name="CSproject" column="CSproject" />
		<property name="CSurl" column="CSurl" />

	</class>
</hibernate-mapping>
