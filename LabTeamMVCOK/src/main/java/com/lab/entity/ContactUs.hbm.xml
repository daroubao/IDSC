<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lab.entity.ContactUs" table="feedback">
		<id name="Fid" column="Fid">
			<generator class="native" />
		</id>
		<property name="Fname" column="Fname" />
		<property name="Femail" column="Femail" />
		<property name="Ftel" column="Ftel" />
		<property name="Finformation" column="Finformation" />
	</class>
</hibernate-mapping>
